{
  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "lib": ["ES6"],
      "outDir": "./dist",
      "rootDirs": [
          "src",
          "tests"
      ],
      "esModuleInterop": true,
      "removeComments": true,
      "noImplicitAny": true,
      "baseUrl": ".",
      "paths": {
          "@/*": ["./src/*"]
      },
  }
}